<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_academic_class" model="ir.sequence">
            <field name="name">Academic Class</field>
            <field name="code">edu.academic.class</field>
            <field name="prefix">CLS-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_academic_section" model="ir.sequence">
            <field name="name">Academic Section</field>
            <field name="code">edu.academic.section</field>
            <field name="prefix">SEC-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_academic_subject" model="ir.sequence">
            <field name="name">Academic Subject</field>
            <field name="code">edu.academic.subject</field>
            <field name="prefix">SUB-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
